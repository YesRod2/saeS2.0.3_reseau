ip a add 10.0.3.2/28 dev eth0
ip link set up eth0

ip a add 172.23.199.254/23 dev eth1
ip link set up eth1

ip r add default via 10.0.3.1 dev eth0

cp shared/rcinterfaces etc/network/interfaces

echo "nameserver 8.8.8.8" > /etc/resolv.conf

dpkg --configure -a
apt update -y && apt upgrade -y
DEBIAN_FRONTEND=noninteractive apt install -y isc-dhcp-server

cp shared/rcdhcpd /etc/dhcp/dhcpd.conf

systemctl start isc-dhcp-server